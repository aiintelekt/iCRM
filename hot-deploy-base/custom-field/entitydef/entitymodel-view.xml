    <view-entity entity-name="CustomFieldSummary" package-name="org.groupfio.custom.field" title="Full Custom field association">
    
		<member-entity entity-alias="CF" entity-name="CustomField"/>
		<member-entity entity-alias="CFG" entity-name="CustomFieldGroup"/>
		<member-entity entity-alias="CFRC" entity-name="CustomFieldRoleConfig"/>
		<member-entity entity-alias="CFVC" entity-name="CustomFieldValueConfig" />
		<alias-all entity-alias="CF" group-by="true"/>
		<alias-all entity-alias="CFRC">
			<exclude field="sequenceNumber" />
		</alias-all>
		<alias-all entity-alias="CFG">
            <exclude field="sequence"/>
            <exclude field="serviceName"/>
            <exclude field="serviceTypeId"/>
            <exclude field="microServiceConfigId"/>
            <exclude field="webhookConfigId"/>
            <exclude field="historicalCapture"/>
            <exclude field="classType"/>
            <exclude field="type"/>
            <exclude field="isUseDynamicEntity"/>
            <exclude field="hide"/>
        </alias-all>
        <alias-all entity-alias="CFVC">
            <exclude field="hide"/>
        </alias-all>
		
		<view-link entity-alias="CF" rel-entity-alias="CFRC" rel-optional="true">
			<key-map field-name="customFieldId" rel-field-name="customFieldId"/>
		</view-link>
		<view-link entity-alias="CF" rel-entity-alias="CFG" rel-optional="true">
			<key-map field-name="groupId" rel-field-name="groupId"/>
		</view-link>
		<view-link entity-alias="CF" rel-entity-alias="CFVC" rel-optional="true" >
			<key-map field-name="customFieldId" rel-field-name="customFieldId" />
		</view-link>
		
	</view-entity>
	
