    <author>None</author>
    <version>1.0</version>
    
    <entity entity-name="CustomFieldSeg${groupId}"
            package-name="org.groupfio.custom.field"
            title="CustomFieldSeg${groupId} Entity">
            
        <field name="groupId" type="id-ne"></field>    
		<field name="customFieldId" type="id-ne"></field>
		<field name="partyId" type="id-ne"></field>
		
		<field name="groupActualValue" type="very-long"></field>
		<field name="inceptionDate" type="date-time"></field>
		
		<prim-key field="groupId"/>
		<prim-key field="customFieldId"/>
		<prim-key field="partyId"/>
      	
    </entity>
    
    <entity entity-name="CustomFieldSegTrk${groupId}"
            package-name="org.groupfio.custom.field"
            title="CustomFieldSegTrk${groupId} Entity">
            
        <field name="groupId" type="id-ne"></field>    
		<field name="customFieldId" type="id-ne"></field>
		<field name="partyId" type="id-ne"></field>
		<field name="valueSeqNum" type="numeric"></field>
		
		<field name="entryDate" type="date-time"></field>
		<field name="exitDate" type="date-time"></field>
		
		<field name="groupActualValue" type="very-long"></field>
		
		<prim-key field="groupId"/>
		<prim-key field="customFieldId"/>
		<prim-key field="partyId"/>
		<prim-key field="valueSeqNum"/>
      	
    </entity>
	
</entitymodel>
