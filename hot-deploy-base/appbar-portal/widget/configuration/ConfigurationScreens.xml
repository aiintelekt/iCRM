    <screen name="FindAppBar">
        <section>
            <actions>
                <set field="titleProperty" value="FindAppBar" global="true"/>
                <set field="sectionName" value="appbar-portal" global="true"/>
                <script location="component://appbar-portal/webapp/appbar-portal/WEB-INF/actions/appbar/FindAppBar.groovy"/>
            </actions>
            <widgets>
                <decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
                    <decorator-section name="top-content">
                        <platform-specific>
                        <html>
							<html-template location="component://appbar-portal/webapp/appbar-portal/appbar/listAppBar.ftl"/>
                        </html>
                        </platform-specific>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>
