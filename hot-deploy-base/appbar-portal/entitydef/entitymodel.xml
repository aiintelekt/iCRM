		<field name="defaultMessage" type="id-mlong" />
		<prim-key field="appBarId"/>
		<prim-key field="appBarTypeId"/>
		
		<relation rel-entity-name="AppBarType" fk-name="AB_TY_FK" type="one">
			<key-map field-name="appBarTypeId"/>
		</relation>
		<index name="AB_ABI_ABS_ABTI">
            <index-field name="appBarId"/>
            <index-field name="appBarTypeId"/>
            <index-field name="appBarStatus"/>
        </index>
	</entity>
	
