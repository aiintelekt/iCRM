		<field name="appBarTypeUilabel" type="id-mlong" />
		<prim-key field="appBarTypeId"/>
	</entity>
	
	<entity entity-name="AppBar" package-name="org.fio.entity.appbar">
		<field name="appBarId" type="id-mlong" />
		<field name="appBarTypeId" type="id-mlong" />
		<field name="appBarName" type="name" />
		<field name="appBarNameUilabel" type="name" />
		<field name="appBarDescription" type="id-very-long" />
		<field name="appBarService" type="id-vlong" />
		<field name="appBarStatus" type="id-mlong" />
		<field name="appBarSecurityGroup" type="id-vlong" />
		<field name="appBarDirection" type="id-mlong" />
		<field name="appBarHeight" type="id-int" />
		<field name="appBarWidth" type="id-int" />
		<field name="appBarMaxElements" type="id-int" />
		<field name="appBarAccessLevel" type="id-mlong" />
		<field name="appBarDataLevel" type="id-mlong" />
		<field name="appBarIsServiceBased" type="indicator" />
		<field name="createdBy" type="long-varchar" />
		<field name="modifiedBy" type="long-varchar" />
		<field name="approvedBy" type="long-varchar" />
		<field name="approvedDate" type="date-time" />
		<field name="autoApprove" type="indicator" />
		<field name="createdOn" type="date-time" />
		<field name="modifiedOn" type="date-time" />
