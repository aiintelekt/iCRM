	<entity entity-name="AppBarUserPreference" package-name="org.fio.entity.appbar">
		<field name="appBarId" type="id-mlong" />
		<field name="appBarTypeId" type="id-mlong" />
		<field name="userLoginId" type="long-varchar" />
		<field name="appBarElementId" type="id-mlong" />
		<field name="appBarElementSeqNum" type="id-int" />
