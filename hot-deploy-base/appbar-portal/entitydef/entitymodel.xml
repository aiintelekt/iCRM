		<prim-key field="appBarTypeId"/>
		<prim-key field="userLoginId"/>
		<prim-key field="appBarElementId"/>
		<relation rel-entity-name="AppBar" fk-name="AB_FK2" type="one">
			<key-map field-name="appBarId"/>
		</relation>
		<relation rel-entity-name="AppBarType" fk-name="AB_TY_FK3" type="one">
