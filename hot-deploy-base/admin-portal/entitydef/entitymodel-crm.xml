<?xml version="1.0" encoding="UTF-8"?>
<entitymodel
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
	<!-- ========================================================= -->
	<!-- ======================== Defaults ======================= -->
	<!-- ========================================================= -->
	<title>Entity of admin-portal Component</title>
	<description>None</description>
	<copyright></copyright>
	<version></version>

	<extend-entity entity-name="PartyRelationship">
		<field name="partyRelAssocId" type="id" />
	</extend-entity>
	<extend-entity entity-name="PartyContactMechPurpose">
		<field name="partyRelAssocId" type="id" />
	</extend-entity>
	<entity entity-name="PartyRelationshipAssoc"
		package-name="org.ofbiz.party.party"
		title="Party Relationship Association Entity">
		<field name="partyRelAssocId" type="id-ne"></field>
		<field name="assocSeqId" type="numeric"></field>
		<field name="assocTypeId" type="id-vlong"></field>
		<field name="assocId" type="id-ne"></field>
		<field name="solicitationStatus" type="indicator"></field>
		<field name="restrictExpired" type="indicator"></field>
		<prim-key field="partyRelAssocId" />
		<prim-key field="assocSeqId" />
		<prim-key field="assocTypeId" />
		<index name="ASSOC" >
			<index-field name="assocId" />
		</index>
		<index name="PARTY_ASSOC" >
			<index-field name="partyRelAssocId" />
		</index>
	</entity>
	<entity entity-name="PartyRelationshipAssocType"
		package-name="org.ofbiz.party.party"
		title="Party Relationship Association Type Entity">
		<field name="assocTypeId" type="id-vlong"></field>
		<field name="assocTypeName" type="long-varchar"></field>
		<prim-key field="assocTypeId" />
	</entity>

	<extend-entity entity-name="PartyRelationship">
		<field name="createdByUserLoginId" type="id-vlong" />
		<field name="isMarketable" type="indicator" />
		<index name="PARTY_REL_INDEX_04">
			<index-field name="partyIdTo" />
			<index-field name="partyRelationshipTypeId" />
		</index>
		<index name="PARTY_REL_INDEX_05">
			<index-field name="partyIdFrom" />
			<index-field name="partyRelationshipTypeId" />
		</index>
		<index name="PARTY_REL_INDEX_06">
			<index-field name="partyIdTo" />
			<index-field name="roleTypeIdTo" />
			<index-field name="partyRelationshipTypeId" />
		</index>
	</extend-entity>

	<extend-entity entity-name="PartySupplementalData">
		<field name="source" type="id-long"></field>
		<field name="partyFirstName" type="name" />
		<field name="partyLastName" type="name" />
		<field name="lastCallBackDate" type="date" />
		<field name="lastContactDate" type="date" />
	</extend-entity>

	<extend-entity entity-name="EmplPositionReportingStruct">
		<field name="emplTeamId" type="id-long"></field>
		<relation type="one" fk-name="EPRS_ETID" rel-entity-name="EmplTeam">
       	 <key-map field-name="emplTeamId" rel-field-name="emplTeamId"/>
      </relation>
	</extend-entity>

	<entity entity-name="EmplTeam"
		package-name="org.ofbiz.humanres.position"
		title="Employee Team Entity">

		<field name="emplTeamId" type="id-long"></field>
		<field name="teamName" type="description" />
		<field name="countryGeoId" type="id-ne" />
		<field name="description" type="description" />
		<field name="isActive" type="indicator" />

		<prim-key field="emplTeamId" />

	</entity>

	<extend-entity entity-name="UserLogin">
		<field name="countryGeoId" type="id"></field>
	</extend-entity>

	<entity entity-name="CountryEnumeration"
		package-name="org.ofbiz.humanres.position"
		title="CountryEnumeration Entity">

		<field name="enumTypeId" type="id-ne"></field>
		<field name="countryCode" type="id-ne" />
		<field name="listType" type="id-long" />
		<field name="sequenceId" type="id" />
		<field name="description" type="description" />

		<prim-key field="enumTypeId" />
		<prim-key field="countryCode" />

	</entity>

	<extend-entity entity-name="EmplPosition">
		<field name="countryGeoId" type="id"></field>
		<field name="city" type="id"></field>
	</extend-entity>
	<extend-entity entity-name="NoteData">

		<field name="noteType" type="id-ne"></field>
		<field name="callBackDate" type="date"></field>
		<field name="subProduct" type="id-ne"></field>

	</extend-entity>
	
	<entity entity-name="WorkEffortLog"
            package-name=""
            title="WorkEffortLog Entity">
		<field name="workEffortLogId" type="id-ne"></field>
		<field name="workEffortId" type="id-ne" />
		<field name="outcomeId" type="very-long"/>
		<field name="companyId" type="id-ne"/>
        <field name="updatedBy" type="id-ne"/>
      	<prim-key field="workEffortLogId"/>
      	<prim-key field="workEffortId"/>
    </entity>
    
    <entity entity-name="LeadStatus"
            package-name=""
            title="LeadStatus Entity">
		<field name="leadId" type="id-ne"></field>
		<field name="statusId" type="id-ne" />
		<field name="description" type="id-ne" />
		<field name="seqId" type="id-ne" />
		<field name="previousStatus" type="id-ne"/>
		<field name="statusNote" type="id-ne"/>
		<field name="reasonId" type="id-long-ne"/>
		<field name="reasonNote" type="very-long"/>
      	<prim-key field="leadId"/>
      	<prim-key field="seqId"/>
      	<prim-key field="statusId"/>
    </entity>
	   
</entitymodel>