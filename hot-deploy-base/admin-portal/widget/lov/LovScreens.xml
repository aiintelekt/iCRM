<?xml version="1.0" encoding="UTF-8"?>
       
<screens xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-screen.xsd">
	
    <screen name="FindLov">
        <section>
            <actions>
                <set field="titleProperty" value="FindLov" global="true"/>
                <set field="sectionName" value="admin-portal" global="true"/>
                <property-map resource="AdminPortalUiLabels" map-name="uiLabelMap" global="true"/>
                <script location="component://admin-portal/webapp/admin-portal/WEB-INF/actions/lov/FindLov.groovy"/>
            </actions>
            <widgets>
				<decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
					<decorator-section name="top-content">
						<platform-specific><html><html-template location="component://admin-portal/webapp/admin-portal/lov/find/find_lov.ftl"/></html></platform-specific> 
	              	</decorator-section>
					<decorator-section name="bottom-content">
						<include-screen name="ListLov"/>
	              	</decorator-section>
	          	</decorator-screen>
            </widgets>
        </section>
    </screen>
    <screen name="ListLov">
        <section>
        	<actions>
				
            </actions>
        	<widgets>
                <platform-specific><html><html-template location="component://admin-portal/webapp/admin-portal/lov/find/list_lov.ftl"/></html></platform-specific>
            </widgets>
        </section>
    </screen>
    
    <screen name="ViewLov">
        <section>
            <actions>
                <set field="titleProperty" value="ViewLov" global="true"/>
                <set field="sectionName" value="admin-portal" global="true"/>
                <set field="isView" value="Y" global="true"/>
                <property-map resource="AdminPortalUiLabels" map-name="uiLabelMap" global="true"/>
                <script location="component://admin-portal/webapp/admin-portal/WEB-INF/actions/lov/ViewLov.groovy"/>
            </actions>
             <widgets>
                <decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
                    <decorator-section name="top-content">
                        <platform-specific>
                                <html>
                                    <html-template location="component://admin-portal/webapp/admin-portal/lov/view/view_lov.ftl"/>
                                </html>
                        </platform-specific>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>
    
	<screen name="CreateLov">
		<section>
			<actions>
			  <set field="titleProperty" value="CreateLov" global="true"/>
			  <set field="sectionName" value="admin-portal" global="true"/>
			  <script location="component://admin-portal/webapp/admin-portal/WEB-INF/actions/lov/CreateLov.groovy"/>
			</actions>
			<widgets>
				<decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
					<decorator-section name="top-content">
						<platform-specific>
							<html>
								<html-template location="component://admin-portal/webapp/admin-portal/lov/create/create_lov.ftl"/>
							</html>
						</platform-specific>
					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
	</screen>
	
	<screen name="UpdateLov">
		<section>
			<actions>
			  <set field="titleProperty" value="UpdateLov" global="true"/>
			  <set field="sectionName" value="admin-portal" global="true"/>
			  <script location="component://admin-portal/webapp/admin-portal/WEB-INF/actions/lov/ViewLov.groovy"/>
			</actions>
			<widgets>
			<section>
			<condition>
				<if-compare field="haveDataPermission" operator="equals" value="Y" type="String"/>
			</condition>
			<widgets>
				<decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
					<decorator-section name="top-content">
						<platform-specific>
							<html>
								<html-template location="component://admin-portal/webapp/admin-portal/lov/update/update_lov.ftl"/>
							</html>
						</platform-specific>
					</decorator-section>
				</decorator-screen>
				</widgets>
	        	<fail-widgets>
					<decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
						<decorator-section name="top-content">
							<label style="h3">${uiLabelMap.permissionDenied}</label>
						</decorator-section>
					</decorator-screen>
				</fail-widgets>
	        </section>
			</widgets>
		</section>
	</screen>
	
	<screen name="ViewLovDetail">
		<section>
			<widgets>
				<platform-specific>
					<html>
						<html-template location="component://admin-portal/webapp/admin-portal/lov/view/lov_detail.ftl"/>
