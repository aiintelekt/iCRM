<?xml version="1.0" encoding="UTF-8"?>
<screens xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-screen.xsd">
		<screen name="auditLog">
		<section>
			<actions>
				<set field="titleProperty" value="Audit Logs" global="true" />
				<set field="sectionName" value="admin-portal" global="true" />
				<property-map resource="AdminPortalUiLabels" map-name="uiLabelMap" global="true" />
				<script location="component://admin-portal/webapp/admin-portal/WEB-INF/actions/auditLog/auditLog.groovy" />
			</actions>
			<widgets>
				<decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
					<decorator-section name="top-content">
						<platform-specific>
							<html>
								<html-template location="component://admin-portal/webapp/admin-portal/auditSetup/auditLog.ftl" />
							</html>
						</platform-specific>
					</decorator-section>
					<decorator-section name="bottom-content">
						<include-screen name="ListAudit" />
					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
	</screen>
	<screen name="ListAudit">
		<section>
			<actions>
			</actions>
			<widgets>
				<platform-specific>
					<html>
						<html-template location="component://admin-portal/webapp/admin-portal/auditSetup/list_audit_log.ftl" />
					</html>
				</platform-specific>
			</widgets>
		</section>
	</screen>
</screens>