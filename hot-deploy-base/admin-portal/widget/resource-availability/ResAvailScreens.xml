<?xml version="1.0" encoding="UTF-8"?>
       
<screens xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-screen.xsd">
	
    <screen name="FindResAvail">
        <section>
            <actions>
                <set field="titleProperty" value="FindResAvail" global="true"/>
                <set field="sectionName" value="admin-portal" global="true"/>
                <property-map resource="AdminPortalUiLabels" map-name="uiLabelMap" global="true"/>
                <script location="component://admin-portal/webapp/admin-portal/WEB-INF/actions/resource-availability/FindResAvail.groovy"/>
            </actions>
            <widgets>
				<decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
					<decorator-section name="top-content">
						<platform-specific><html><html-template location="component://admin-portal/webapp/admin-portal/resource-availability/find/find_availability.ftl"/></html></platform-specific> 
	              	</decorator-section>
					<decorator-section name="bottom-content">
						<include-screen name="ListResAvail"/>
	              	</decorator-section>
	          	</decorator-screen>
            </widgets>
        </section>
    </screen>
    <screen name="ListResAvail">
        <section>
        	<actions>
				
            </actions>
        	<widgets>
                <platform-specific><html><html-template location="component://admin-portal/webapp/admin-portal/resource-availability/find/list_availability.ftl"/></html></platform-specific>
            </widgets>
        </section>
    </screen>
    
    <screen name="ViewResAvail">
        <section>
            <actions>
                <set field="titleProperty" value="ViewResAvail" global="true"/>
                <set field="sectionName" value="admin-portal" global="true"/>
                <set field="isView" value="Y" global="true"/>
                <property-map resource="AdminPortalUiLabels" map-name="uiLabelMap" global="true"/>
                <script location="component://admin-portal/webapp/admin-portal/WEB-INF/actions/resource-availability/ViewResAvail.groovy"/>
                
            </actions>
             <widgets>
                <decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
                    <decorator-section name="top-content">
                        <platform-specific>
                                <html>
                                    <html-template location="component://admin-portal/webapp/admin-portal/resource-availability/view/view_availability.ftl"/>
                                </html>
                        </platform-specific>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>
    
	<screen name="CreateResAvail">
		<section>
			<actions>
			  <set field="titleProperty" value="CreateResAvail" global="true"/>
			  <set field="sectionName" value="admin-portal" global="true"/>
			  <script location="component://admin-portal/webapp/admin-portal/WEB-INF/actions/resource-availability/CreateResAvail.groovy"/>
			</actions>
			<widgets>
				<decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
					<decorator-section name="top-content">
						<platform-specific>
							<html>
								<html-template location="component://admin-portal/webapp/admin-portal/resource-availability/create/create_availability.ftl"/>
							</html>
						</platform-specific>
					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
	</screen>
	<screen name="UpdateResAvail">
		<section>
			<actions>
			  <set field="titleProperty" value="UpdateResAvail" global="true"/>
			  <set field="sectionName" value="admin-portal" global="true"/>
			  <script location="component://admin-portal/webapp/admin-portal/WEB-INF/actions/resource-availability/ViewResAvail.groovy"/>
			</actions>
			<widgets>
			<section>
			<condition>
				<if-compare field="haveDataPermission" operator="equals" value="Y" type="String"/>
			</condition>
			<widgets>
				<decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
					<decorator-section name="top-content">
						<platform-specific>
							<html>
								<html-template location="component://admin-portal/webapp/admin-portal/resource-availability/update/update_availability.ftl"/>
							</html>
						</platform-specific>
					</decorator-section>
					<!-- <decorator-section name="bottom-content">
						<include-screen name="UpdateSubscriptionAdditional"/>
	              	</decorator-section> -->
				</decorator-screen>
				</widgets>
	        	<fail-widgets>
					<decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
						<decorator-section name="top-content">
							<label style="h3">${uiLabelMap.permissionDenied}</label>
						</decorator-section>
					</decorator-screen>
				</fail-widgets>
	        </section>
			</widgets>
		</section>
	</screen>
	
	<screen name="ViewResAvailDetail">
		<section>
			<widgets>
				<platform-specific>
					<html>
						<html-template location="component://admin-portal/webapp/admin-portal/resource-availability/view/availability_detail.ftl"/>
					</html>
				</platform-specific>
			</widgets>
		</section>
	</screen>
	
	<screen name="AdministrationInfo">
		<section>
			<actions>
                
            </actions>
			<widgets>
				<platform-specific>
					<html>
						<html-template location="component://admin-portal/webapp/admin-portal/resource-availability/administration_info.ftl"/>
					</html>
				</platform-specific>
			</widgets>
		</section>
	</screen>
	
	<screen name="BackupResourceSetup">
		<section>
			<actions>
			  <set field="titleProperty" value="BackupResourcessetup" global="true"/>
			  <set field="sectionName" value="admin-portal" global="true"/>
			</actions>
			<widgets>
				<decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
					<decorator-section name="top-content">
						<platform-specific>
							<html>
								<html-template location="component://admin-portal/webapp/admin-portal/resource-availability/create/backup_configuration.ftl"/>
							</html>
						</platform-specific>
					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
	</screen>
	
	
</screens>
