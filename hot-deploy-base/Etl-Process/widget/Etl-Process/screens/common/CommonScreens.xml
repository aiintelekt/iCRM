	<screen name="PortalBaseSetup">
		<section>
			<actions>
				
				<property-map resource="FioDashboardUiLabels" map-name="uiLabelMap" global="true"/>
                <property-map resource="CommonUiLabels" map-name="uiLabelMap" global="true"/>
                <property-map resource="PartyUiLabels" map-name="uiLabelMap" global="true"/>
                <property-map resource="opentaps" map-name="configProperties" global="true" />
                
                <!-- The default (global) shortcut icon -->
                <set field="layoutSettings.shortcutIcon" value="/opentaps_images/favicon.ico" global="true"/>
                
                <set field="initialLocale" type="String" value="${groovy:parameters?.userLogin?.lastLocale?.substring(0,2)}" default-value="${groovy:locale?.toString()?.substring(0,2)?:'en'}"/>
				<!-- <set field="layoutSettings.javaScripts[+0]" value="/images/prototypejs/prototype.js" global="true"/> -->
				
				<!-- <set field="layoutSettings.javaScripts[]" value="/images/googleanalytics.js" global="true"/> -->

                <!-- Get the store VisualTheme -->
                <!-- <set field="visualThemeId" from-field="productStore.visualThemeId" default-value="EC_DEFAULT"/> -->
                <set field="visualThemeId" value="METRONIC"/>
                <service service-name="getVisualThemeResources">
                    <field-map field-name="visualThemeId"/>
                    <field-map field-name="themeResources" from-field="layoutSettings"/>
                </service>
                <set field="layoutSettings" from-field="themeResources" default-value="${layoutSettings}" global="true"/>
                <set field="headerTemplateLocation" from-field="layoutSettings.VT_HDR_TMPLT_LOC[0]" default-value="component://ecommerce/webapp/ecommerce/includes/header.ftl" global="true"/>
                <set field="footerTemplateLocation" from-field="layoutSettings.VT_FTR_TMPLT_LOC[0]" default-value="component://ecommerce/webapp/ecommerce/includes/footer.ftl" global="true"/>
				<script location="component://Etl-Process/webapp/Etl-Process/WEB-INF/actions/includes/MainDecorator.groovy" />
			</actions>
			<widgets>
				<decorator-screen name="ProtalMainLayout">
					<decorator-section name="left-sidebar-content">
						<decorator-section-include name="left-sidebar-content"/>
					</decorator-section>
					<decorator-section name="top-content">
						<decorator-section-include name="top-content"/>
					</decorator-section>
					<decorator-section name="bottom-content">
						<decorator-section-include name="bottom-content"/>
					</decorator-section>
					<decorator-section name="extra-bottom-content">
						<decorator-section-include name="extra-bottom-content"/>
					</decorator-section>
					<decorator-section name="body-end">
						<decorator-section-include name="body-end"/>
					</decorator-section>
					<decorator-section name="quick-sidebar-content">
						<decorator-section-include name="quick-sidebar-content"/>
					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
	</screen>
