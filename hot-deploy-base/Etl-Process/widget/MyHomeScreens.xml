	<screen name="etlUsers">
		<section>
			<actions>
			<property-map resource="Etl-ProcessUiLabels" map-name="uiLabelMap" global="true"/>
				 <set field="titleProperty" value="${uiLabelMap.modelList}" /> 
				 <script location="component://Etl-Process/webapp/Etl-Process/WEB-INF/actions/includes/MainDecorator.groovy" /> 
				 <script location="component://Etl-Process/webapp/Etl-Process/WEB-INF/actions/includes/etlProcess.groovy" />
			</actions>
			<widgets>
				<decorator-screen name="main-decorator"
					location="${parameters.mainDecoratorLocation}">
					<decorator-section name="body">
						<platform-specific>
							<html>
								<html-template
									location="component://Etl-Process/webapp/Etl-Process/shared/etlExecuteModel.ftl" />
							</html>
						</platform-specific>
					</decorator-section>
				</decorator-screen>

			</widgets>
		</section>
	</screen>
